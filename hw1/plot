import numpy as np
import matplotlib.pyplot as plt


m_s = 62.0 / 35
b_s = 18.0 / 35

s_x = [0, 2, 3, 4]
s_y = [1, 3, 6, 8]

l_x = range(6)
l_y = []
for i in range(len(l_x)):
	l_y.append(m_s * l_x[i] + b_s)

plt.plot(s_x, s_y, 'ro')
plt.plot(l_x, l_y)
plt.axis([0, 5, 0, 10])
plt.show()

